{
  "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "adminEmailAddress": { "value": "<ChangeMe>" },
    "developerIpAddress": { "value": "<ChangeMe>" },
    "environmentName": { "value": "int" },
    "hostNameSslStates": {
      "value": []
    },
    "servicePlanSkuTier": { "value": "Standard" },
    "servicePlanSkuSize": { "value": "S1" },
    "servicePlanSkuFamily": { "value": "S" },
    "servicePlanSkuCapacity": { "value": 1 },
    "siteName": { "value": "securityessentials" },
    "sqlserverAdminLoginUserName": { "value": "<ChangeMe>" },
    "sqlserverAdminLoginPassword": {
      "reference": {
        "keyVault": {
          "id": "[concat('/subscriptions/', parameters('subscriptionId'), '/resourceGroups/SecurityEssentials-Int/providers/Microsoft.KeyVault/vaults/securityessentialsint']"
        },
        "secretName": "SqlAzurePassword"
      }
    },
    "subscriptionId": { "value": "<ChangeMe>" }
  }
}